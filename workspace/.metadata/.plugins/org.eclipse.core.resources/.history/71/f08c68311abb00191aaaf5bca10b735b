package drawpic;
import java.awt.BorderLayout;
import java.awt.Container;

//import java.awt.*;
import javax.swing.*;

public class SizeTool {
	JComboBox<String> sizeTool;
	public SizeTool() {
		sizeTool = new JComboBox<String>();
		sizeTool.addItem("最小");
		sizeTool.addItem("小");
		sizeTool.addItem("大");
		sizeTool.addItem("最大");
		sizeTool.setSelectedIndex(1);
	}
}

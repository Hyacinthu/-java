public class AllPanel {
	JPanel westPanel,northPanel,centerPanel;//定义三个区
	public AllPanel() {
		westPanel = new JPanel(new 	GridLayout(7,2));//七行两列
		JMenuBar temp = new JMenuBar();
		temp.setLayout(new FlowLayout(FlowLayout.LEFT));//设置菜单栏属性
		
	}
	
}
